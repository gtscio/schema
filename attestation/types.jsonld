{
	"@context": {
		"@version": 1.1,
		"@protected": true,
		"twin-attestation": "https://schema.twindev.org/attestation/",
		"schema": "http://schema.org/",
		"id": "@id",
		"type": "@type",
		"AuditableInformation": "twin-attestation:AuditableInformation",
		"AuditableProof": "twin-attestation:AuditableProof",
		"dateTransferred": {
			"@id": "twin-attestation:dateTransferred",
			"@type": "schema:Date"
		},
		"ownerIdentity": {
			"@id": "twin-attestation:ownerIdentity",
			"@type": "schema:identifier"
		},
		"holderIdentity": {
			"@id": "twin-attestation:holderIdentity",
			"@type": "schema:identifier"
		},
		"attestationObject": {
			"@id": "twin-attestation:attestationObject",
