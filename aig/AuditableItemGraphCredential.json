{
	"type": "object",
	"properties": {
		"@context": {
			"anyOf": [
				{
					"type": "string",
					"const": "https://schema.twindev.org/aig/"
				},
				{
					"type": "array",
					"minItems": 1,
					"items": [
						{
							"type": "string",
							"const": "https://schema.twindev.org/aig/"
						}
					],
					"additionalItems": {
						"type": "string"
					}
				}
			],
			"description": "JSON-LD Context."
		},
		"type": {
			"type": "string",
			"const": "AuditableItemGraphPatchCredential",
			"description": "JSON-LD Type."
		},
		"dateCreated": {
			"type": "string",
			"description": "The date/time of when the changeset was created."
		},
		"userIdentity": {
			"type": "string",
			"description": "The user identity that created the changes."
		},
		"signature": {
			"type": "string",
			"description": "The signature for the changeset."
		},
		"hash": {
			"type": "string",
			"description": "The signature for the changeset."
		},
		"integrity": {
			"type": "string",
			"description": "The integrity data for this changeset, encrypted."
		}
	},
	"required": ["@context", "type", "dateCreated", "userIdentity", "signature", "hash"],
	"additionalProperties": false,
	"description": "The data stored immutably for the graph in a verifiable credential."
}
